[  {    "type": "POST",    "url": "/inspections/add",    "title": "add inspection.",    "name": "InspectionAPI_add",    "group": "Admin_User_Application",    "header": {      "fields": {        "Header": [          {            "group": "Header",            "type": "Content-Type",            "optional": false,            "field": "application/json",            "description": ""          }        ]      }    },    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "Object",            "optional": false,            "field": "InformationInspection",            "description": "<p>(need to be add in the body)</p>"          }        ]      }    },    "examples": [      {        "title": "request example",        "content": "endpoint: /inspections/add\nheader : Content-Type:application/json\n\nbody:\n {\n    \"idRestaurant\": 00,\n    \"restaurant\": {\n      \"name\": \"XXX\",\n      \"borough\": \"XXX\",\n      \"buildingnum\": \"000\",\n      \"street\": \"XXX\",\n      \"zipcode\": \"00\",\n      \"phone\": \"00\",\n      \"cuisineType\": \"XXX\"\n    },\n    \"inspectionDate\": \"XXX\",\n    \"violationCode\": \"XXX\",\n    \"violationDescription\": \"XXX\",\n    \"criticalFlag\": \"XXX\",\n    \"score\": 00,\n    \"grade\": \"XXX\"\n  }",        "type": "json"      }    ],    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "Response",            "description": "<p>element added</p>"          }        ]      }    },    "version": "0.0.0",    "filename": "doc/docApi.js",    "groupTitle": "Admin_User_Application"  },  {    "type": "GET",    "url": "/inspections/read",    "title": "Read inspection.",    "name": "InspectionAPI_read",    "group": "User_Application",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "limit",            "description": "<p>limit the result number by default is 10</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "idRestaurant",            "description": "<p>add filter by idRestaurant</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "idInspection",            "description": "<p>add filter by idInspection</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "date",            "description": "<p>add filter by year</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "type",            "description": "<p>add filter by cook type</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "name",            "description": "<p>add filter by restaurant name</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "borough",            "description": "<p>add filter by borough</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "score",            "description": "<p>sort the result in term of the score and order given in param</p>"          }        ]      }    },    "examples": [      {        "title": "request example",        "content": "/inspections?limit=2&date=2013",        "type": "json"      }    ],    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "_id",            "description": "<p>id inspection</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "idRestaurant",            "description": "<p>id restaurant inspected</p>"          },          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "restaurant",            "description": "<p>information about the restaurant inspected</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "inspectionDate",            "description": "<p>inspected date</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "violationCode",            "description": "<p>code violation</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "violationDescription",            "description": "<p>description of the violation</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "score",            "description": "<p>inspection score</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "grade",            "description": "<p>marks avualuting the inspection</p>"          }        ]      }    },    "version": "0.0.0",    "filename": "doc/docApi.js",    "groupTitle": "User_Application"  },  {    "type": "GET",    "url": "/inspections/stats",    "title": "Give some stats for a restaurant given",    "name": "InspectionAPI_stats",    "group": "User_Application",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "idRestaurant",            "description": "<p>the restaurant id</p>"          }        ]      }    },    "examples": [      {        "title": "request example",        "content": "/inspections/stats?idRestaurant=41217775",        "type": "json"      }    ],    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "avgScore",            "description": "<p>Average score received</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "scoreMax",            "description": "<p>Score max received</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "numberInspec",            "description": "<p>number inspections</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "avgPerYear",            "description": "<p>Average score per year</p>"          }        ]      }    },    "version": "0.0.0",    "filename": "doc/docApi.js",    "groupTitle": "User_Application"  },  {    "type": "GET",    "url": "/inspections/verify",    "title": "Verify restaurant already saved in the database.",    "name": "InspectionAPI_verify",    "group": "User_Application",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "zipcode",            "description": "<p>the restaurant zipcode</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "phone",            "description": "<p>the restaurant phone number</p>"          }        ]      }    },    "examples": [      {        "title": "request example",        "content": "/inspections/verify?phone=2129829166&zipcode=10003",        "type": "json"      }    ],    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Boolean",            "optional": false,            "field": "found",            "description": "<p>give information if the restaurant exist</p>"          },          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "restaurant",            "description": "<p>return information about the restaurant</p>"          },          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "idRestautnat",            "description": "<p>return the id resaurant</p>"          }        ]      }    },    "version": "0.0.0",    "filename": "doc/docApi.js",    "groupTitle": "User_Application"  }]
